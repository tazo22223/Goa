# Creating a dictionary with at least five key-value pairs
my_dictionary = {
    "name": "John",
    "age": 25,
    "city": "Tbilisi",
    "country": "Georgia",
    "occupation": "Engineer"
}

# Iterating over the dictionary and printing each key with its corresponding value
for key, value in my_dictionary.items():
    print(f"{key}: {value}")
